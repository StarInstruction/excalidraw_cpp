    {
      "id": "OsQeqaaI84oeF_dctknS6",
      "type": "text",
      "x": 41.966334780608804,
      "y": 1938.2049598105443,
      "width": 1623.5989990234375,
      "height": 225,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 2,
      "strokeStyle": "solid",
