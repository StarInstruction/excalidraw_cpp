      "type": "text",
      "x": 587.2248372433055,
      "y": 1338.7727370662424,
      "width": 1326.5992431640625,
      "height": 90,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
